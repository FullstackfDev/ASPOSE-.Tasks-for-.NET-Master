
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>Microsoft Project File Format Solution</h1>
				<h4>Use our Apps to view & convert project files or build applications via On Premise or Cloud-based APIs to manage Microsoft Project tasks.</h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_tasks-brand.png" alt="Aspose.Tasks" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeTasks"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_viewer-app.png" alt="Aspose.Tasks Product Family" />
					</div>
					<a href="/tasks/viewer"><span class="spanclass">@Model.Resources["AsposeTasks"] @Model.Resources["AsposeProductFor"]  <em>@Model.Resources["Viewerh4"] </em></span><em>View an image representation of your Microsoft Project document, from anywhere at anytime, while you are on the move. Supports MPP, XML.</em></a>
				</div>

			</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.Tasks for .NET" />
					</div>
					<a href="/tasks/conversion"><span class="spanclass">@Model.Resources["AsposeTasks"] @Model.Resources["AsposeProductFor"]  <em>Conversion</em></span><em>Drop or upload your Microsoft Project file and convert it to Primavera, raster image, PDF or many other formats.</em></a>
				</div>
			</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_metadata-app.png" alt="Aspose.Tasks for .NET" />
					</div>
					<a href="/tasks/metadata"><span class="spanclass">@Model.Resources["AsposeTasks"] @Model.Resources["AsposeProductFor"]<em>Metadata</em></span><em>Use onilne App to view or edit Metadata properties of Microsoft Project files.</em></a>
				</div>

			</div>


		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>